{
  "name": "razzle-examples-with-afterjs",
  "version": "3.0.0",
  "keywords": [
    "react",
    "reactjs",
    "argon",
    "argon-react",
    "dashboard",
    "dashboard-react",
    "argon-dashboard",
    "argon-dashboard-react"
  ],
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
  },
  "dependencies": {
    "@jaredpalmer/after": "latest",
    "chart.js": "^2.8.0",
    "classnames": "^2.2.6",
    "express": "^4.16.2",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "nouislider": "^13.1.5",
    "razzle": "^3.0.0",
    "react": "^16.2.0",
    "react-chartjs-2": "^2.7.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^8.0.0"
  },
  "devDependencies": {
    "razzle-plugin-scss": "^3.0.0"
  }
}
